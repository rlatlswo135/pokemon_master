{
    "extends": ["airbnb", "plugin:prettier/recommended"],
    "env": {
        "browser": true,
        "node": true
    },
    "ignorePatterns": ["build", "dist", "public"],
    "parser": "@babel/eslint-parser",
    "rules": {
        "react/jsx-filename-extension": [0],
        "import/extensions": "off"
    }
}
